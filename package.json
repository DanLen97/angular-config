{
  "name": "angular-config",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:config": "ng build --project config --configuration=production",
    "watch": "ng build --watch --configuration development",
    "watch:config": "ng build --watch --project config --configuration development",
    "test": "ng test",
    "test:config": "ng test --project config",
    "lint": "ng lint",
    "release": "npm run build:config && changeset publish"
  },
  "private": true,
  "workspaces": [
    "projects/config"
  ],
  "dependencies": {
    "@angular/animations": "^17.3.5",
